    <groundspeak:placed_by><![CDATA[geopart]]></groundspeak:placed_by>
    <groundspeak:owner id="4394508"><![CDATA[geopart]]></groundspeak:owner>
    <groundspeak:type>Traditional Cache</groundspeak:type>
