   <groundspeak:cache id="5256306" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Kaplicka]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[honza.h]]></groundspeak:placed_by>
