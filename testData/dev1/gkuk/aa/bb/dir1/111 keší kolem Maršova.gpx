  <extensions>
   <groundspeak:cache id="3973695" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[U partyzana]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[michal&atka]]></groundspeak:placed_by>
