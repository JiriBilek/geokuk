   <groundspeak:cache id="2287345" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Tisnovske Geopivko #3 - Strom]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[berx.cz]]></groundspeak:placed_by>
