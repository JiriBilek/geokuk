  <type>Geocache|Unknown Cache</type>
  <extensions>
   <groundspeak:cache id="2811619" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Samochodnij kombajn SK-4]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[hauskaa]]></groundspeak:placed_by>
    <groundspeak:owner id="963949"><![CDATA[hauskaa]]></groundspeak:owner>
