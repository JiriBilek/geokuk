  <type>Geocache|Traditional Cache</type>
  <extensions>
   <groundspeak:cache id="888785" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Kostel sv. Vaclava a sv. Jana Krtitele]]></groundspeak:name>
