 <wpt lat="49.332033" lon="16.431217">
  <time>2015-03-02T00:00:00.000</time>
  <name>GC5NPJE</name>
  <desc><![CDATA[Recakuv biotop - CWG smenarna by berx.cz (1.5/2)]]></desc>
  <link href="http://www.geocaching.com/seek/cache_details.aspx?guid=f6688346-a9aa-49e5-be37-c175c75cbbf6">
    <text><![CDATA[Recakuv biotop - CWG smenarna by berx.cz]]></text>
  </link>
  <sym>Geocache</sym>
  <type>Geocache|Traditional Cache</type>
  <extensions>
   <groundspeak:cache id="4853810" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Recakuv biotop - CWG smenarna]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[berx.cz]]></groundspeak:placed_by>
    <groundspeak:owner id="4167287"><![CDATA[berx.cz]]></groundspeak:owner>
    <groundspeak:type>Traditional Cache</groundspeak:type>
