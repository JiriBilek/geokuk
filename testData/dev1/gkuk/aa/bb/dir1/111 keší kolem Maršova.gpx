   <groundspeak:cache id="5106235" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Volna hodina]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[berx.cz]]></groundspeak:placed_by>
    <groundspeak:owner id="4167287"><![CDATA[berx.cz]]></groundspeak:owner>
    <groundspeak:type>Unknown Cache</groundspeak:type>
