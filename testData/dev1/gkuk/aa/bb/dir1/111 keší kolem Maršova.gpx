   <groundspeak:cache id="4340291" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[III. Mlyn Velka Bites - Knolluv mlyn]]></groundspeak:name>
