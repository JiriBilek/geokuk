  <extensions>
   <groundspeak:cache id="2004565" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Olomoucke podzemi]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[miroslav.turek]]></groundspeak:placed_by>
    <groundspeak:owner id="1609122"><![CDATA[miroslav.turek]]></groundspeak:owner>
