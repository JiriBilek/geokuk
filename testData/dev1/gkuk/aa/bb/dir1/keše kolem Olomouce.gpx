  <extensions>
   <groundspeak:cache id="4842654" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[sbohem Olomouc (drive-in)]]></groundspeak:name>
