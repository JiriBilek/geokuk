   <groundspeak:cache id="592188" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Pevnost Olomouc - Fort predsunute obrany, Tab.vrch]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[MilanoXTeam]]></groundspeak:placed_by>
    <groundspeak:owner id="502974"><![CDATA[MilanoXTeam]]></groundspeak:owner>
    <groundspeak:type>Traditional Cache</groundspeak:type>
