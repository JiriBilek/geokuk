   <groundspeak:cache id="1457181" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[ZST Olomouc hl.n.]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[honz@net]]></groundspeak:placed_by>
    <groundspeak:owner id="1955260"><![CDATA[honz@net]]></groundspeak:owner>
    <groundspeak:type>Unknown Cache</groundspeak:type>
