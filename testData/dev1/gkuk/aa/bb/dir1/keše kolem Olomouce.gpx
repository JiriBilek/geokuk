   <groundspeak:cache id="1153982" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[PROTIPOVODNOVA OPATRENI OLOMOUCE  1. CAST – HORKA]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[HUGO III]]></groundspeak:placed_by>
    <groundspeak:owner id="747941"><![CDATA[HUGO III]]></groundspeak:owner>
    <groundspeak:type>Traditional Cache</groundspeak:type>
