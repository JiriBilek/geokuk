   <groundspeak:cache id="3890246" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Rimane v Olomouci]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[J.S.Jasanek]]></groundspeak:placed_by>
    <groundspeak:owner id="2287743"><![CDATA[J.S.Jasanek]]></groundspeak:owner>
