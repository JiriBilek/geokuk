   <groundspeak:cache id="5314674" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Den valecnych veteranu - Olomouc 2015]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[sedento]]></groundspeak:placed_by>
    <groundspeak:owner id="1730767"><![CDATA[sedento]]></groundspeak:owner>
    <groundspeak:type>Event Cache</groundspeak:type>
    <groundspeak:container>Other</groundspeak:container>
