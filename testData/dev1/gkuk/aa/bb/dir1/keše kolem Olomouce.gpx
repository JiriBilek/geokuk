   <groundspeak:cache id="1346103" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Dominikani Olomouc]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[Schrek]]></groundspeak:placed_by>
    <groundspeak:owner id="1741928"><![CDATA[Schrek]]></groundspeak:owner>
    <groundspeak:type>Multi-cache</groundspeak:type>
