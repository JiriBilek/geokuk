   <groundspeak:cache id="3586525" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Nova tramvaj v Olomouci]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[Jelen]]></groundspeak:placed_by>
    <groundspeak:owner id="574128"><![CDATA[Jelen]]></groundspeak:owner>
    <groundspeak:type>Traditional Cache</groundspeak:type>
