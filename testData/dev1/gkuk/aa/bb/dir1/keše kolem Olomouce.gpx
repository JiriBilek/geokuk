 <wpt lat="49.585083" lon="17.347117">
  <time>2012-08-31T00:00:00.000</time>
  <name>GC3CRXZ</name>
  <desc><![CDATA[MM12 - Olomouc jungle by OLGK & Pipak & Miroslav.Turek (1.5/2)]]></desc>
  <link href="http://www.geocaching.com/seek/cache_details.aspx?guid=b87f26b2-54e8-40f7-a049-ea7a852ebf50">
    <text><![CDATA[MM12 - Olomouc jungle by OLGK & Pipak & Miroslav.Turek]]></text>
  </link>
  <sym>Geocache</sym>
  <type>Geocache|Event Cache</type>
  <extensions>
   <groundspeak:cache id="2740897" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[MM12 - Olomouc jungle]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[OLGK & Pipak & Miroslav.Turek]]></groundspeak:placed_by>
    <groundspeak:owner id="1468772"><![CDATA[OLGK & Pipak & Miroslav.Turek]]></groundspeak:owner>
    <groundspeak:type>Event Cache</groundspeak:type>
    <groundspeak:container>Not chosen</groundspeak:container>
