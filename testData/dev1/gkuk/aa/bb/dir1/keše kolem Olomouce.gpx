   <groundspeak:cache id="2523802" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Den valecnych veteranu - vojensky hrbitov Olomouc]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[sedento]]></groundspeak:placed_by>
