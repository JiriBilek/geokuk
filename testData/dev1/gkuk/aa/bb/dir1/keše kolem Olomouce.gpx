   <groundspeak:cache id="1722952" available="False" archived="True" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Aquapark Olomouc]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[Buránci]]></groundspeak:placed_by>
    <groundspeak:owner id="3055448"><![CDATA[Buránci]]></groundspeak:owner>
    <groundspeak:type>Traditional Cache</groundspeak:type>
