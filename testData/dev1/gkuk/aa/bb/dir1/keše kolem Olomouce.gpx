  <extensions>
   <groundspeak:cache id="2599019" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Pevnost Olomouc-zeleznicni fort c.1 Cernovir]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[Tesman Pavel]]></groundspeak:placed_by>
