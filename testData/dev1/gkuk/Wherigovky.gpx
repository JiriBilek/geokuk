   <groundspeak:cache id="1717640" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[sumperska sifrovacka]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[quattrocz]]></groundspeak:placed_by>
    <groundspeak:owner id="875504"><![CDATA[quattrocz]]></groundspeak:owner>
    <groundspeak:type>Wherigo Cache</groundspeak:type>
