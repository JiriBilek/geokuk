  <extensions>
   <groundspeak:cache id="4264291" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[Financ]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[Selmice and ZCh]]></groundspeak:placed_by>
    <groundspeak:owner id="534973"><![CDATA[Selmice and ZCh]]></groundspeak:owner>
