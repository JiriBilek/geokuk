    <text><![CDATA[MX12(2A) Sipkovana by HanackeExpres]]></text>
  </link>
  <sym>Geocache</sym>
  <type>Geocache|Wherigo Cache</type>
  <extensions>
   <groundspeak:cache id="4895515" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0">
    <groundspeak:name><![CDATA[MX12(2A) Sipkovana]]></groundspeak:name>
    <groundspeak:placed_by><![CDATA[HanackeExpres]]></groundspeak:placed_by>
    <groundspeak:owner id="3151965"><![CDATA[HanackeExpres]]></groundspeak:owner>
